%h2
  Login
= form_for @user_session do |f|
  -#= f.error_messages
  = render 'shared/error_messages', :target => @user_session
  %table
    %tr.form_field.required
      %td.form_label
        = f.label :login
      %td
        = f.text_field :login

    %tr.form_field.required
      %td.form_label
        = f.label :password
      %td
        = f.password_field :password

  = f.submit "Login"
